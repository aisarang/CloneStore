@section scriptsbyend{
    <script src="~/Scripts/W/owl.carousel.js"></script>
    <script src="~/Scripts/W/jquery.mouseoverbox.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {

            $("#owl_cart_shoes .item   span,#owl_cart_shirt .item   span,#owl_cart_pants .item   span ").each(function () {

                var ihtml = '';
                ihtml += '  <div style="border: 1px solid rgb(0, 0, 0); border-image: none; width: 146px; height: 146px; margin-right: 10px; float: left;">';

                ihtml += '?';

                ihtml += ' </div>';

                $(this).mouseOverTip({
                    width: '150px',
                    height: '150px',
                    outlineBoxId: "mycart_box",
                    boxId: "savedproduct_box_xiuTipBox",
                    content: ihtml
                });
            });


            $('#owl_cart_shirt').owlCarousel({
                items: 7,
                navigation: true,
                itemsDesktop: [1199, 7],
                itemsDesktopSmall: [979, 7],
                itemsTablet: [768, 7],
                itemsMobile: [479, 7],
                navigationText: ["", ""]
            });


            $('#owl_cart_shoes').owlCarousel({
                items: 7,
                navigation: true,
                itemsDesktop: [1199, 7],
                itemsDesktopSmall: [979, 7],
                itemsTablet: [768, 7],
                itemsMobile: [479, 7],
                navigationText: ["", ""]
            });

            $('#owl_cart_pants').owlCarousel({
                items: 2,
                navigation: true,
                itemsDesktop: [1199, 2],
                itemsDesktopSmall: [979, 2],
                itemsTablet: [768, 2],
                itemsMobile: [479, 2],
                navigationText: ["", ""]
            });


            $("#owl_cart_shirt .item  span img").live('click', function (i) {
                var src = $(this).attr("src");
                $("#gallary_shirt_img").attr("src", src);
            });

            $("#owl_cart_shoes .item  span img").live('click', function (i) {
                var src = $(this).attr("src");
                $("#gallary_shoes_img").attr("src", src);
            });

            $("#owl_cart_pants .item  span img").live('click', function (i) {
                var src = $(this).attr("src");
                $("#gallary_pants_img").attr("src", src);
            });

        });
    </script>

}
